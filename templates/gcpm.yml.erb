---
<% if @config_dir != "" -%>
config_dir: <%= @config_dir %>
<% end -%>
<% if @oauth_file != "" -%>
oauth_file: <%= @oauth_file %>
<% end -%>
<% if @service_account_file != "" -%>
service_account_file: <%= @service_account_file %>
<% end -%>
project: <%= @project %>
zone: <%= @zone %>
max_cores: <%= @max_cores %>
machines:
<% @machines.each do |machine| -%>
  - core: <%= machine["core"] %>
    mem: <%= machine["mem"] %>
    disk: <%= machine["disk"] %>
    max: <%= machine["max"] %>
    idle: <%= machine["idle"] %>
    image: <%= machine["image"] %>
<% end -%>
<% if @static_wns.size != 0 -%>
static_wns:
<% @static_wns.each do |s| -%>
  - <%= s %>
<% end -%>
<% end -%>
prefix: <%= @prefix %>
preemptible: <%= @preemptible %>
off_timer: <%= @off_timer %>
<% if @network_tag.size != 0 -%>
network_tag:
<% @network_tag.each do |tag| -%>
  - <%= tag %>
<% end -%>
<% end -%>
reuse: <%= @reuse %>
interval: <%= @interval %>
<% if @head_info != "" -%>
head_info: <%= @head_info %>
<% end -%>
<% if @head != "" -%>
head: <%= @head %>
<% end -%>
port: <%= @port %>
domain: <%= @domain %>
owner: <%= @owner %>
wait_cmd: <%= @wait_cmd %>
bucket: <%= @bucket %>
storageClass: <%= @storageClass %>
<% if @location != "" -%>
location: <%= @location %>
<% end -%>
<% if @log_file != "" -%>
log_file: <%= @log_file %>
<% end -%>
log_level: <%= @log_level %>
